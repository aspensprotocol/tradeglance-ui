{
  "name": "tradeglance-backend",
  "version": "1.0.0",
  "description": "Backend server for TradeGlance UI gRPC interactions",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "proto:generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/proto --ts_proto_opt=esModuleInterop=true,outputServices=grpc,outputClientImpl=grpc ./proto/*.proto"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-protobuf": "^3.21.2",
    "grpc-web": "^1.5.0",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "ts-proto": "^1.156.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
